{
  "version": "2",
  "templates": [
    {
      "categories": [
        "LoRaWAN"
      ],
      "description": "This project deploys a LoRaWAN gateway with Basic™ Station Packet Forward protocol using docker.",
      "env": [
        {
          "default": "SX1301",
          "label": "MODEL",
          "name": "MODEL",
          "description": "The model is defined depending on the version of the LoRa concentrator: SX1301, SX1302 and SX1303."
        },
        {
          "name": "GW_GPS",
          "label": "GW_GPS",
          "select": [
            {
              "text": "Enable",
              "value": "true"
            },
            {
              "text": "Disable",
              "value": "false"
            }
          ],
          "description": "Enables GPS"
        },
        {
          "default": "17",
          "label": "GW_RESET_GPIO:",
          "name": "GW_RESET_GPIO:",
          "description": "GPIO number that resets (Broadcom pin number)"
        },
        {
          "default": "0",
          "label": "GW_ENABLE_GPIO",
          "name": "GW_ENABLE_GPIO",
          "description": "GPIO number that enables power (by pulling HIGH) to the concentrator (Broadcom pin number)"
        },
        {
          "name": "TTN_STACK_VERSION",
          "label": "TTN_STACK_VERSION",
          "default": "3",
          "select": [
            {
              "text": "3",
              "value": "3"
            },
            {
              "text": "2",
              "value": "2"
            }
          ],
          "description": "If using TTN, version of the stack. It can be either 2 (TTNv2) or 3 (TTS Community Edition)"
        },
        {
          "default": "eu1",
          "label": "TTN_REGION",
          "name": "TTN_REGION",
          "description": "Region of the TTN server to use, possible values: eu1, nam1, au1; when using TTNv2, possible values are eu, us, in, and au"
        },
        {
          "default": "change this to your geteway EUI",
          "label": "GW_EUI",
          "name": "GW_ID",
          "description": "TTN Gateway EUI"
        },
        {
          "default": "change this to your own TC key from TTN",
          "label": "TC_KEY",
          "name": "TC_KEY",
          "description": "Unique TTN Gateway Key used for TTS Community Edition pasted from TTN console"
        }
      ],
      "image": "xoseperez/basicstation:aarch64-latest",
      "logo": "https://avatars.githubusercontent.com/u/706487?v=4",
      "name": "BasicStation_Packet_Forwarder",
      "platform": "linux",
      "restart_policy": "unless-stopped",
      "title": "BasicStation Packet Forwarder",
      "type": 1,
      "network": "host",
      "privileged": true,
      "hostname": "basicstation"
    },
    {
      "categories": [
        "Home Automation"
      ],
      "description": "Node-RED is a flow-based development tool for visual programming.",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "default": "False",
          "label": "Enable the projects feature",
          "name": "NODE_RED_ENABLE_PROJECTS",
          "select": [
            {
              "text": "Enable",
              "value": "true"
            },
            {
              "text": "Disable",
              "value": "false"
            }
          ]
        }
      ],
      "image": "nodered/node-red:latest",
      "logo": "https://nodered.org/about/resources/media/node-red-icon-2.png",
      "name": "NodeRed",
      "platform": "linux",
      "ports": [
        "1880:1880/tcp"
      ],
      "restart_policy": "unless-stopped",
      "title": "NodeRed",
      "type": 1,
      "volumes": [
        {
          "bind": "/data",
          "container": "/node_red_data"
        }
      ],
      "network": "host",
      "hostname": "NodeRed"
    },
    {
      "categories": [
        "LoRaWAN_Chirpstack"
      ],
      "description": "The ChirpStack open-source LoRaWAN Network Server stack provides open-source components for LoRaWAN networks.",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "default": "EU868",
          "label": "Network Server Band",
          "name": "NETWORK_SERVER__BAND__NAME",
          "description": "Frequency channels used by Chirpstack for uplink transmissions, valid values are AU915, EU868, US915 ... "
        }
      ],
      "logo": "data:image/png;base64,",
      "name": "ChirpStack_Network_Server",
      "platform": "linux",
      "repository": {
        "stackfile": "docker-compose-template.yml",
        "url": "https://github.com.cnpmjs.org/Sheng-IoT/chirpstack-docker.git"
      },
      "title": "ChirpStack_Network_Server",
      "type": 3
    },
    {
      "categories": [
        "LoRaWAN"
      ],
      "description": "The Things Stack LNS establishes a data connection between a LoRa Basics™ Station gateway and a Network Server (in this case, The Things Stack).",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "label": "TTS DOMAIN",
          "name": "TTS_DOMAIN",
          "description": "set this to the IP or domain name of the host you will be using to access the stack "
        }
      ],
      "logo": "data:image/png;base64,",
      "name": "The Things Stack LNS",
      "platform": "linux",
      "repository": {
        "stackfile": "docker-compose-template.yml",
        "url": "https://github.com.cnpmjs.org/Sheng-IoT/the-things-stack-docker.git"
      },
      "title": "The Things Stack LNS",
      "type": 3
    },
    {
      "categories": [
        "LoRaWAN"
      ],
      "description": "Dockerize the rak_common_for_gateway project, which aim to help users to use the RAK Raspberry Pi Developer Gateway more easily.",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "name": "RAK_Concentrator_Model",
          "label": "RAK Concentrator Model",
          "select": [
            {
              "text": "RAK2247(USB)",
              "value": "4"
            },
            {
              "text": "RAK2247(SPI)",
              "value": "5"
            },
            {
              "text": "RAK7248(SPI) no LTE (RAK2287(SPI) + raspberry pi)",
              "value": "7"
            },
            {
              "text": "RAK7248(SPI) with LTE (RAK2287(SPI) + LTE + raspberry pi)",
              "value": "8"
            },
            {
              "text": "RAK2287(USB)",
              "value": "9"
            },
            {
              "text": "RAK5146(USB)",
              "value": "10"
            },
            {
              "text": "RAK5146(SPI)",
              "value": "11"
            },
            {
              "text": "RAK5146(SPI) with LTE",
              "value": "12"
            }
          ],
          "description": "Please select your concentrator/gateway model"
        },
        {
          "name": "GATEWAY_RESET_GPIO",
          "default": "17",
          "label": "GATEWAY RESET GPIO",
          "description": "GPIO number that resets (Broadcom pin number)"
        },
        {
          "name": "Server_Address",
          "label": "Server Address",
          "default": "nam1.cloud.thethings.network",
          "description": "If using TTN, server address can be eu1.cloud.thethings.network, nam1.cloud.thethings.network, and au1.cloud.thethings.network; or you can use enter your own server address"
        },
        {
          "name": "TTN_Frequency_Plan",
          "label": "TTN Frequency Plan",
          "select": [
            {
              "text": "as_915_921",
              "value": "as_915_921"
            },
            {
              "text": "as_915_928",
              "value": "as_915_928"
            },
            {
              "text": "as_917_920",
              "value": "as_917_920"
            },
            {
              "text": "as_920_923",
              "value": "as_920_923"
            },
            {
              "text": "au_915_928",
              "value": "au_915_928"
            },
            {
              "text": "cn_470_510",
              "value": "cn_470_510"
            },
            {
              "text": "eu_433",
              "value": "eu_433"
            },
            {
              "text": "eu_863_870",
              "value": "eu_863_870"
            },
            {
              "text": "in_865_867",
              "value": "in_865_867"
            },
            {
              "text": "kr_920_923",
              "value": "kr_920_923"
            },
            {
              "text": "ru_864_870",
              "value": "ru_864_870"
            },
            {
              "text": "us_902_928",
              "value": "us_902_928"
            }
          ],
          "description": "Please select the frequency plan. Check LoRaWAN regional parameters document for more details"
        },
        {
          "name": "USB_Port",
          "default": "ttyACM0",
          "label": "USB Port",
          "description": "If you are using a USB concentrator module, you can change this to the USB port, for example, ttyUSB0 and ttyUSB1"
        }
      ],
      "image": "sheng2216/udp_packet_forwarder:0.2",
      "logo": "data:image/png;base64,",
      "name": "UDP_Packet_Forwarder",
      "platform": "linux",
      "restart_policy": "unless-stopped",
      "title": "UDP Packet Forwarder",
      "type": 1,
      "network": "host",
      "privileged": true,
      "hostname": "UDP"
    }
  ]
}
