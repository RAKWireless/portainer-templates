{
  "version": "2",
  "templates": [
    {
      "categories": [
        "LoRaWAN"
      ],
      "description": "This project deploys a LoRaWAN gateway with Basic™ Station Packet Forward protocol using docker.",
      "env": [
        {
          "default": "SX1301",
          "label": "MODEL",
          "name": "MODEL",
          "description": "The model is defined depending on the version of the LoRa concentrator: SX1301, SX1302 and SX1303."
        },
        {
          "name": "GW_GPS",
          "label": "GW_GPS",
          "select": [
            {
              "text": "Enable",
              "value": "true"
            },
            {
              "text": "Disable",
              "value": "false"
            }
          ],
          "description": "Enables GPS"
        },
        {
          "default": "17",
          "label": "GW_RESET_GPIO:",
          "name": "GW_RESET_GPIO:",
          "description": "GPIO number that resets (Broadcom pin number)"
        },
        {
          "default": "0",
          "label": "GW_ENABLE_GPIO",
          "name": "GW_ENABLE_GPIO",
          "description": "GPIO number that enables power (by pulling HIGH) to the concentrator (Broadcom pin number)"
        },
        {
          "name": "TTN_STACK_VERSION",
          "label": "TTN_STACK_VERSION",
          "default": "3",
          "select": [
            {
              "text": "3",
              "value": "3"
            },
            {
              "text": "2",
              "value": "2"
            }
          ],
          "description": "If using TTN, version of the stack. It can be either 2 (TTNv2) or 3 (TTS Community Edition)"
        },
        {
          "default": "eu1",
          "label": "TTN_REGION",
          "name": "TTN_REGION",
          "description": "Region of the TTN server to use, possible values: eu1, nam1, au1; when using TTNv2, possible values are eu, us, in, and au"
        },
        {
          "default": "change this to your geteway EUI",
          "label": "GW_EUI",
          "name": "GW_ID",
          "description": "TTN Gateway EUI"
        },
        {
          "default": "change this to your own TC key from TTN",
          "label": "TC_KEY",
          "name": "TC_KEY",
          "description": "Unique TTN Gateway Key used for TTS Community Edition pasted from TTN console"
        }
      ],
      "image": "xoseperez/basicstation:v2",
      "logo": "https://avatars.githubusercontent.com/u/706487?v=4",
      "name": "BasicStation_Packet_Forwarder",
      "platform": "linux",
      "restart_policy": "unless-stopped",
      "title": "BasicStation Packet Forwarder",
      "type": 1,
      "network": "host",
      "privileged": true,
      "hostname": "basicstation"
    },
    {
      "categories": [
        "Home Automation"
      ],
      "description": "Node-RED is a flow-based development tool for visual programming.",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "default": "False",
          "label": "Enable the projects feature",
          "name": "NODE_RED_ENABLE_PROJECTS",
          "select": [
            {
              "text": "Enable",
              "value": "true"
            },
            {
              "text": "Disable",
              "value": "false"
            }
          ]
        }
      ],
      "image": "nodered/node-red:latest",
      "logo": "https://nodered.org/about/resources/media/node-red-icon-2.png",
      "name": "NodeRed",
      "platform": "linux",
      "ports": [
        "1880:1880/tcp"
      ],
      "restart_policy": "unless-stopped",
      "title": "NodeRed",
      "type": 1,
      "volumes": [
        {
          "bind": "/data",
          "container": "/node_red_data"
        }
      ],
      "network": "host",
      "hostname": "NodeRed"
    },
    {
      "categories": [
        "LoRaWAN_Chirpstack"
      ],
      "description": "The ChirpStack open-source LoRaWAN Network Server stack provides open-source components for LoRaWAN networks.",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "default": "EU868",
          "label": "Network Server Band",
          "name": "NETWORK_SERVER__BAND__NAME",
          "description": "Frequency channels used by Chirpstack for uplink transmissions, valid values are AU915, EU868, US915 ... "
        }
      ],
      "logo": "data:image/png;base64,",
      "name": "ChirpStack_Network_Server",
      "platform": "linux",
      "repository": {
        "stackfile": "docker-compose-template.yml",
        "url": "https://github.com.cnpmjs.org/Sheng-IoT/chirpstack-docker.git"
      },
      "title": "ChirpStack_Network_Server",
      "type": 3
    },
    {
      "categories": [
        "LoRaWAN"
      ],
      "description": "The Things Stack LNS establishes a data connection between a LoRa Basics™ Station gateway and a Network Server (in this case, The Things Stack).",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "100",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "Europe/London",
          "label": "Time Zone",
          "name": "TZ"
        },
        {
          "label": "TTS DOMAIN",
          "name": "TTS_DOMAIN",
          "description": "set this to the IP or domain name of the host you will be using to access the stack "
        }
      ],
      "logo": "data:image/png;base64,",
      "name": "The Things Stack LNS",
      "platform": "linux",
      "repository": {
        "stackfile": "docker-compose-template.yml",
        "url": "https://github.com.cnpmjs.org/Sheng-IoT/the-things-stack-docker.git"
      },
      "title": "The Things Stack LNS",
      "type": 3
    },
    {
      "categories": [
        "LoRaWAN"
      ],
      "description": "Dockerize the rak_common_for_gateway project, which aim to help users to use the RAK Raspberry Pi Developer Gateway more easily.",
      "env": [
        {
          "name": "MODEL",
          "label": "RAK gateway model",
          "select": [
            {
              "text": "RAK7243/RAK7244(no LTE)",
              "value": "RAK7244"
            },
            {
              "text": "RAK7243C/RAK7244C(with LTE)",
              "value": "RAK7244C"
            },
            {
              "text": "RAK7246",
              "value": "RAK7246"
            },
            {
              "text": "RAK7246G",
              "value": "RAK7246G"
            },
            {
              "text": "RAK7248(no LTE)",
              "value": "RAK7248"
            },
            {
              "text": "RAK7248C(with LTE)",
              "value": "RAK7248C"
            },
            {
              "text": "RAK7271",
              "value": "RAK7271"
            },
            {
              "text": "RAK7371",
              "value": "RAK7371"
            }
          ],
          "description": "RAKwireless Developer gateway model. Once defined, we take care of all the concentrator configurations and you can skip to option <Interface for gateway EUI>"
        },
        {
          "name": "MODEL",
          "label": "RAK WisLink LPWAN module",
          "select": [
            {
              "text": "RAK831(SPI only)",
              "value": "RAK831"
            },
            {
              "text": "RAK2245(SPI only)",
              "value": "RAK2245"
            },
            {
              "text": "RAK2247(USB)",
              "value": "RAK2247_USB"
            },
            {
              "text": "RAK2247(SPI)",
              "value": "RAK2247_SPI"
            },
            {
              "text": "RAK833(USB)",
              "value": "RAK833_USB"
            },
            {
              "text": "RAK833(SPI)",
              "value": "RAK833_SPI"
            },
            {
              "text": "RAK2246(SPI only)",
              "value": "RAK2246"
            },
            {
              "text": "RAK2287(SPI/USB)",
              "value": "RAK2287"
            },
            {
              "text": "RAK5146(SPI/USB)",
              "value": "RAK5146"
            }
          ],
          "description": "If RAK gateway model is not defined, you can define either <RAKwireless WisLink LPWAN module> or <Semtech concentrator>. Once defined this virable, also need to specify other features of the module, for example, <Concentrator interface>,<HAS GPS>,<HAS LTE>."
        },
        {
          "name": "CONCENTRATOR",
          "label": "Semtech concentrator",
          "select": [
            {
              "text": "SX1301",
              "value": "SX1301"
            },
            {
              "text": "SX1302",
              "value": "SX1302"
            },
            {
              "text": "SX1303",
              "value": "SX1303"
            },
            {
              "text": "SX1308",
              "value": "SX1308"
            }
          ],
          "description": "If RAK gateway model is not defined, you can define either <RAKwireless WisLink LPWAN module> or <Semtech concentrator>. Once defined this virable, also need to specify other features of the module, for example, <Concentrator interface>,<HAS GPS>,<HAS LTE>."
        },
        {
          "name": "INTERFACE",
          "label": "Concentrator interface",
          "default": "SPI",
          "select": [
            {
              "text": "SPI",
              "value": "SPI"
            },
            {
              "text": "USB",
              "value": "USB"
            }
          ],
          "description": "If MODEL is defined it will get the interface type from it, otherwise defaults to SPI"
        },
        {
          "name": "HAS_GPS",
          "label": "Has GPS",
          "default": "1",
          "select": [
            {
              "text": "True",
              "value": "1"
            },
            {
              "text": "False",
              "value": "0"
            }
          ],
          "description": "If MODEL is defined it will get this from it, otherwise defaults to 1 (with GPS)"
        },
        {
          "name": "HAS_LTE",
          "label": "Has LTE",
          "default": "0",
          "select": [
            {
              "text": "True",
              "value": "1"
            },
            {
              "text": "False",
              "value": "0"
            }
          ],
          "description": "If MODEL id defined it will get this from it, otherwise defaults to 0 (without LTE)"
        },
        {
          "name": "RESET_GPIO",
          "default": "17",
          "label": "GATEWAY RESET GPIO",
          "description": "GPIO number that resets (Broadcom pin number, if not defined, it's calculated based on the RESET_PIN)"
        },
        {
          "name": "POWER_EN_GPIO",
          "default": "0",
          "label": "the GPIO enables power",
          "description": "GPIO number that enables power (by pulling HIGH) to the concentrator (Broadcom pin number). 0 means not required"
        },
        {
          "name": "RADIO_DEV",
          "label": "Where the concentrator is connected to",
          "description": "/dev/spidev0.0 for SPI concentrators, /dev/ttyACM0 for USB concentrators.Don't set it if you don't know what this means"
        },
        {
          "name": "GPS_DEV",
          "label": "Where the GPS is connected to",
          "description": "/dev/ttyAMA0 except when HAS_LTE is 1, in this case it will default to /dev/i2c-1. Don't set it if you don't know what this means"
        },
        {
          "name": "GATEWAY_EUI_NIC",
          "label": "Interface for gateway EUI",
          "default": "eth0",
          "description": "Interface to use when generating the EUI, default to eth0"
        },
        {
          "name": "GATEWAY_EUI",
          "label": "Gateway EUI",
          "description": "Autogenerated from GATEWAY_EUI_NIC if defined or the any of these: eth0, wlan0, usb0"
        },
        {
          "name": "TTN_REGION",
          "label": "TTN region",
          "default": "eu1",
          "description": "If using a TTN server, region of the TTN server to use. Can be eu1, nam1, and au1"
        },
        {
          "name": "SERVER_HOST",
          "label": "Host Server URL",
          "description": "If TTN_REGION is defined it will build the right address for the TTN server"
        },
        {
          "name": "SERVER_PORT",
          "label": "Server port",
          "default": "1700",
          "description": "Port the server is listening to"
        },
        {
          "name": "BAND",
          "label": "TTN Frequency Plan",
          "default": "eu_863_870",
          "select": [
            {
              "text": "as_915_921",
              "value": "as_915_921"
            },
            {
              "text": "as_915_928",
              "value": "as_915_928"
            },
            {
              "text": "as_917_920",
              "value": "as_917_920"
            },
            {
              "text": "as_920_923",
              "value": "as_920_923"
            },
            {
              "text": "au_915_928",
              "value": "au_915_928"
            },
            {
              "text": "cn_470_510",
              "value": "cn_470_510"
            },
            {
              "text": "eu_433",
              "value": "eu_433"
            },
            {
              "text": "eu_863_870",
              "value": "eu_863_870"
            },
            {
              "text": "in_865_867",
              "value": "in_865_867"
            },
            {
              "text": "kr_920_923",
              "value": "kr_920_923"
            },
            {
              "text": "ru_864_870",
              "value": "ru_864_870"
            },
            {
              "text": "us_902_928",
              "value": "us_902_928"
            }
          ],
          "description": "Regional parameters identifier"
        },
        {
          "name": "GPS_LATITUDE",
          "label": "GPS LATITUDE",
          "description": "Report this latitude for the gateway"
        },
        {
          "name": "GPS_LONGITUDE",
          "label": "GPS LONGITUDE",
          "description": "Report this longitude for the gateway"
        },
        {
          "name": "GPS_ALTITUDE",
          "label": "GPS ALTITUDE",
          "description": "Report this altitude for the gateway"
        }
      ],
      "image": "sheng2216/udp_packet_forwarder:xose0.2",
      "name": "UDP_Packet_Forwarder",
      "platform": "linux",
      "restart_policy": "unless-stopped",
      "title": "UDP Packet Forwarder",
      "type": 1,
      "network": "host",
      "privileged": true,
      "hostname": "UDP"
    }
  ]
}
